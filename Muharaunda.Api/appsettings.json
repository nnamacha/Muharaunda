{
  "AllowedHosts": "*",
  "General": {
    "MinAgeInMonth": 3,
    "MobileNumberLength": 10,
    "NumberOfDaysToActivateProfile": 60,
    "MaximumDependentAge": 20,
    "ProfileCreationAutoAuthorisation": false,
    "KeyVault": "https://munharaunda.vault.azure.net/"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithProcessId",
      "WithThread"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Africa\\LogFile\\Munharaunda\\Log.txt",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Africa\\LogFile\\Munharaunda\\Log.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  },
  "AzureAd": {
    "Authority": "https://login.microsoftonline.com/nnamachagmail.onmicrosoft.com",
    "Audience": "api://TodoListService/api/useraccess"

  }
}